				float2 texCoord55 = IN.ase_texcoord4.xy * float2( 1,1 ) + float2( 0,0 );
				float2 panner57 = ( 1.0 * _Time.y * float2( 0,0 ) + texCoord55);
				float4 tex2DNode47 = tex2D( _CloudTexture, panner57 );
				float4 lerpResult50 = lerp( tex2DNode47 , ( 1.0 - tex2DNode47 ) , _InvertCloudAlpha);
				float4 color52 = IsGammaSpace() ? float4(0,0,0,0) : float4(0,0,0,0);
				float4 Cloud79 = ( 0.0 * lerpResult50 * color52 );
				float2 texCoord35 = IN.ase_texcoord4.xy * float2( 1,1 ) + float2( 0,0 );
				float2 panner34 = ( 1.0 * _Time.y * float2( 0,0 ) + texCoord35);
				float4 tex2DNode33 = tex2D( _HexTexture, panner34 );
				float4 lerpResult39 = lerp( tex2DNode33 , ( 1.0 - tex2DNode33 ) , _InvertHexAlpha);
				float4 color43 = IsGammaSpace() ? float4(0,0,0,0) : float4(0,0,0,0);
				float4 Hex45 = ( 0.0 * lerpResult39 * color43 );
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = normalize(ase_worldViewDir);
				float3 ase_worldNormal = IN.ase_texcoord5.xyz;
				float fresnelNdotV59 = dot( ase_worldNormal, ase_worldViewDir );
				float fresnelNode59 = ( _Bias + _Scale * pow( 1.0 - fresnelNdotV59, _Power ) );
